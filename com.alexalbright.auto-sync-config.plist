<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.alexalbright.auto-sync-config</string>
        <key>ServiceDescription</key>
        <string>Auto sync configs to git repository</string>
        <key>ProgramArguments</key>
        <array>
            <string>/bin/zsh</string>
            <string>-c</string>
            <string>/Users/alexalbright/scripts/config-backup/sync_configs.sh</string>
        </array>
        <key>StandardOutPath</key>
        <string>/Users/alexalbright/scripts/logs/com.alexalbright.sync-configs.stdout</string>
        <key>StandardErrorPath</key>
        <string>/Users/alexalbright/scripts/logs/com.alexalbright.sync-configs.stderr</string>
        <key>WorkingDirectory</key>
        <string>/Users/alexalbright/scripts/config-backup</string>
        <key>KeepAlive</key>
        <true/>
     </dict>
  </plist>
